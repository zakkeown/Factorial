[
    (
        name: "mine",
        processor: Source(item: "raw_ore", rate: 2.0),
        footprint: (width: 2, height: 2),
        inventories: (input_capacity: 100, output_capacity: 100),
    ),
    (
        name: "refinery",
        processor: PropertyTransform(
            input: "raw_ore",
            output: "refined_ore",
            property: "quality",
