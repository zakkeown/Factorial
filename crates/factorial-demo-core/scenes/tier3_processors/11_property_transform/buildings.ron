        footprint: (width: 3, height: 3),
        inventories: (input_capacity: 200, output_capacity: 200),
    ),
    (
        name: "storage",
        processor: Demand(items: ["refined_ore"]),
        footprint: (width: 2, height: 2),
        inventories: (input_capacity: 200, output_capacity: 200),
    ),
]
